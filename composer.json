{
    "name": "marwanosama8/full-google-calendar",
    "description": "This is my package full-google-calendar",
    "keywords": [
        "marwanosama8",
        "laravel",
        "full-google-calendar"
    ],
    "homepage": "https://github.com/marwanosama8/full-google-calendar",
    "support": {
        "issues": "https://github.com/marwanosama8/full-google-calendar/issues",
        "source": "https://github.com/marwanosama8/full-google-calendar"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "marwanosama8",
            "email": "marwan.osam8888@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "google/apiclient": "^2.17.0",
        "google/apiclient-services": "^0.376.0",
        "saade/filament-fullcalendar": "^3.0",
        "stevebauman/hypertext": "^1.1"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Marwanosama8\\FullGoogleCalendar\\": "src/",
            "Marwanosama8\\FullGoogleCalendar\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marwanosama8\\FullGoogleCalendar\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marwanosama8\\FullGoogleCalendar\\FullGoogleCalendarServiceProvider"
            ],
            "aliases": {
                "FullGoogleCalendar": "Marwanosama8\\FullGoogleCalendar\\Facades\\FullGoogleCalendar"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}